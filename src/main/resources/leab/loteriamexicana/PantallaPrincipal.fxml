<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="225.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.PantallaPrincipalController">
    <children>
      <Pane prefHeight="280.0" prefWidth="300.0">
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d967f5" height="280.0" layoutX="5.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="3.0" width="290.0" />
            <Button fx:id="btn_Configuraciones" layoutX="20.0" layoutY="90.0" mnemonicParsing="false" onAction="#accionConfiguraciones" prefWidth="260.0" text="Configuraciones">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="btn_NuevoJuego" layoutX="20.0" layoutY="30.0" mnemonicParsing="false" onAction="#accionNuevoJuego" prefWidth="260.0" text="Nuevo Juego">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="btn_Reporte" layoutX="20.0" layoutY="150.0" mnemonicParsing="false" onAction="#accionReporte" prefWidth="260.0" text="Reporte">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="btn_Reporte1" layoutX="20.0" layoutY="210.0" mnemonicParsing="false" onAction="#accionSalir" prefWidth="260.0" text="Salir">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
